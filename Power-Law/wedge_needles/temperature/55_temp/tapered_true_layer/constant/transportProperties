{
    nuMax           nuMax [ 0 2 -1 0 0 0 0 ] 11.267e-3;
    nuMin           nuMin [ 0 2 -1 0 0 0 0 ] 0.40773e-3;
    
    k               k [ 0 2 -1 0 0 0 0 ] 5.13e-3;
    n               n [ 0 0 0 0 0 0 0 ]  0.64;
}
